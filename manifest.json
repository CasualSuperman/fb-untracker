{
	"name": "Facebook Untracker",
	"version": "1.3",
	"description": "Disables Facebook's outbound link tracking.",
	"update_url": "http://tobias104.github.com/fb-untracker/updates.xml",
	"icons": {
		"16": "icon_16.webp",
		"48": "icon_48.webp",
		"128": "icon_128.webp"
	},
	"content_scripts": [
		{
			"matches": ["http://www.facebook.com/*"],
			"run_at": "document_end",
			"js": ["untrack.js"]
		}
	]
}
