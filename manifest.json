{
	"name": "Facebook Untracker",
	"version": "4.0",
	"description": "Disables Facebook's outbound link tracking.",
	"update_url": "https://github.com/CasualSuperman/fb-untracker/raw/master/updates.xml",
	"icons": {
		"16": "icon_16.webp",
		"48": "icon_48.webp",
		"128": "icon_128.webp"
	},
	"content_scripts": [
		{
			"matches": ["*://*.facebook.com/*"],
			"js": ["untrack.js"]
		}
	],
	"manifest_version": 2
}
